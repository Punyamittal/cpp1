#include <iostream>
using namespace std;
int main() {
    cout<<"enter a number";
    int a,d;
    d=0;
    cin>>a;
    for(int i=2;i<=a-1;i++){
        if (a%i==0){
            d=1;
        }
    }
    if(d==0){
        cout<<"it is prime";
    }
    else{
        cout<<"it is not prime";
    }
    
}
